  inherited selTabella: TOracleDataSet
    SQL.Strings = (
      'select '
      '  I073.AZIENDA,'
      '  I073.PROFILO,'
      '  nvl(INIBIZIONE_S,0) INIBIZIONE_S,'
      '  nvl(INIBIZIONE_R,0) INIBIZIONE_R,'
      '  nvl(INIBIZIONE_N,0) INIBIZIONE_N'
      'from   '
      '  (select distinct AZIENDA,PROFILO '
      '   from MONDOEDP.I073_FILTROFUNZIONI'
      '   ) I073,'
      '  (select AZIENDA,PROFILO,count(*) INIBIZIONE_S'
      '   from MONDOEDP.I073_FILTROFUNZIONI '
      '   where INIBIZIONE = '#39'S'#39
      '   and APPLICAZIONE in (:APPLICAZIONE,'#39'FUNWEB'#39')'
      '      group by AZIENDA,PROFILO) I073_S,'
      '  (select AZIENDA,PROFILO,count(*) INIBIZIONE_N'
      '   from MONDOEDP.I073_FILTROFUNZIONI '
      '   where INIBIZIONE = '#39'N'#39
      '   and APPLICAZIONE in (:APPLICAZIONE,'#39'FUNWEB'#39')   '
      '   group by AZIENDA,PROFILO) I073_N,'
      '  (select AZIENDA,PROFILO,count(*) INIBIZIONE_R'
      '   from MONDOEDP.I073_FILTROFUNZIONI '
      '   where INIBIZIONE = '#39'R'#39
      '   and APPLICAZIONE in (:APPLICAZIONE,'#39'FUNWEB'#39')   '
      '   group by AZIENDA,PROFILO) I073_R'
      'where I073.AZIENDA = I073_S.AZIENDA(+)'
      'and I073.PROFILO = I073_S.PROFILO(+)'
      'and I073.AZIENDA = I073_N.AZIENDA(+)'
      'and I073.PROFILO = I073_N.PROFILO(+)'
      'and I073.AZIENDA = I073_R.AZIENDA(+)'
      'and I073.PROFILO = I073_R.PROFILO(+)'
      ':ORDERBY')
